    {
      "name": "benMaruchu",
      "email": "benmaruchu@gmail.com",
      "url": "https://github.com/BenMaruchu"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeTanzania/majifix-service-group/issues",
