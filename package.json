    "@codetanzania/majifix-priority": ">=1.4.4",
    "@lykmapipo/express-common": ">=0.12.2",
    "async": ">=2.6.2",
    "express": ">=4.16.4",
    "lodash": ">=4.17.11",
    "moment": ">=2.24.0",
    "mongoose": "^5.5.0",
    "mongoose-geojson-schemas": ">=0.10.1",
    "mongoose-locale-schema": ">=0.2.1",
    "mongoose-rest-actions": ">=0.24.0",
    "randomcolor": ">=0.5.4"
  },
  "devDependencies": {
    "@benmaruchu/faker": "^4.2.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/travis-cli": "^7.5.2",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "chai": "^4.2.0",
